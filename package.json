{
  "name": "bun_panda",
  "version": "0.1.17",
  "description": "Pandas-inspired DataFrame and Series utilities for Bun/TypeScript.",
  "license": "MIT",
  "homepage": "https://github.com/your-org/bun_panda#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/bun_panda.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/bun_panda/issues"
  },
  "keywords": [
    "dataframe",
    "series",
    "pandas",
    "typescript",
    "bun",
    "data-science"
  ],
  "module": "index.ts",
  "exports": {
    ".": "./index.ts"
  },
  "files": [
    "index.ts",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "type": "module",
  "scripts": {
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit",
    "check": "bun run typecheck && bun run test",
    "bench": "bun run bench:arquero",
    "bench:arquero": "bun run bench/compare.js",
    "bench:io": "bun run bench/io.js",
    "bench:pandas": "python bench/pandas_compare.py",
    "bench:compare:pandas": "bun run bench/compare-pandas.js",
    "bench:gate": "bun run bench/assert-regression.js",
    "bench:gate:io": "bun run bench/assert-io-regression.js",
    "bench:gate:pandas": "bun run bench/assert-pandas-regression.js",
    "bench:refresh-readme": "bun run bench/update-readme.js",
    "bench:ci": "bun run bench:arquero && bun run bench:gate && bun run bench:io && bun run bench:gate:io && bun run bench:pandas && bun run bench:compare:pandas && bun run bench:gate:pandas && bun run bench:refresh-readme"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "arquero": "^8.0.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "parquetjs-lite": "^0.8.7",
    "xlsx": "^0.18.5"
  }
}
